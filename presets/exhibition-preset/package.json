{
  "name": "@manifest-editor/exhibition-preset",
  "version": "2.0.1",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/exhibition-preset"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "prepack": "tsdown --clean"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "7.0.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "^3.2.1",
    "@iiif/helpers": "catalog:",
    "@iiif/parser": "catalog:",
    "@iiif/presentation-3": "^2.2.3",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@manifest-editor/components": "workspace:*",
    "@manifest-editor/creator-api": "workspace:*",
    "@manifest-editor/creators": "workspace:*",
    "@manifest-editor/editor-api": "workspace:*",
    "@manifest-editor/editors": "workspace:*",
    "@manifest-editor/manifest-preset": "workspace:*",
    "@manifest-editor/shell": "workspace:*",
    "exhibition-viewer": "^1.0.10",
    "react": "catalog:",
    "react-aria-components": "^1.12",
    "react-dom": "catalog:",
    "react-iiif-vault": "catalog:",
    "tailwind-merge": "^2.3.0",
    "tiny-invariant": "^1.3.3",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@manifest-editor/ui": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "postcss-import": "^16.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tiny-invariant": "^1.3.3",
    "tsdown": "0.16.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
