{
  "name": "@manifest-editor/manifest-preset",
  "version": "1.0.0",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/manifest-preset"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.tsx",
    "dev": "tsup src/index.tsx --watch",
    "typecheck": "tsc --noEmit",
    "prepack": "tsup src/index.tsx"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "6.1.8",
    "@manifest-editor/components": "1.0.0",
    "@manifest-editor/shell": "1.0.0",
    "@manifest-editor/editors": "1.0.0",
    "@manifest-editor/creators": "1.0.0",
    "react-iiif-vault": "^1.1.1",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@iiif/presentation-3": "^2.2.2",
    "@iiif/parser": "^2.1.0",
    "@iiif/helpers": "^1.1.0"
  },
  "devDependencies": {
    "@manifest-editor/ui": "1.0.0",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@repo/eslint-config": "0.0.0",
    "tiny-invariant": "^1.3.3",
    "tsup": "^8.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
