{
  "name": "@manifest-editor/manifest-preset",
  "version": "1.4.3",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/manifest-preset"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./components": {
      "require": {
        "types": "./dist/components.d.ts",
        "default": "./dist/components.cjs"
      },
      "import": {
        "types": "./dist/components.d.ts",
        "default": "./dist/components.js"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "prepack": "tsdown --clean"
  },
  "dependencies": {
    "@iiif/helpers": "^1.4.0",
    "@iiif/parser": "^2.2.0",
    "@iiif/presentation-3": "^2.2.3",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@manifest-editor/editor-api": "workspace:*",
    "@manifest-editor/components": "workspace:*",
    "@manifest-editor/creators": "workspace:*",
    "@manifest-editor/editors": "workspace:*",
    "@manifest-editor/shell": "workspace:*",
    "react-aria-components": "^1.12",
    "tailwind-merge": "^2.3.0",
    "react": "^18.3.1",
    "zustand": "^4.5.2",
    "react-dom": "^18.3.1",
    "react-iiif-vault": "https://pkg.csb.dev/digirati-co-uk/react-iiif-vault/commit/40a6d11c/react-iiif-vault",
    "styled-components": "6.1.8"
  },
  "devDependencies": {
    "@manifest-editor/ui": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "tiny-invariant": "^1.3.3",
    "tsdown": "^0.14.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "22aa937fcbd97344b3d350b75e46bb8887c56d47"
}
