{
  "name": "@manifest-editor/iiif-preview-server",
  "version": "2.0.0",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/iiif-preview-server"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "nodeDependencies": [
    "http",
    "net",
    "url",
    "timers"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./netlify": {
      "import": "./dist/netlify.mjs",
      "require": "./dist/netlify.cjs"
    },
    "./next-app-router": {
      "import": "./dist/next-app-router.mjs",
      "require": "./dist/next-app-router.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "lint": "publint",
    "prepack": "tsdown"
  },
  "optionalDependencies": {
    "next": "^16"
  },
  "dependencies": {
    "@iiif/helpers": "catalog:",
    "@netlify/blobs": "^8.0.1",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@iiif/presentation-3": "^2.2.3",
    "@repo/eslint-config": "workspace:*",
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "tsdown": "0.16.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
