{
  "name": "manifest-editor",
  "version": "2.0.2",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/manifest-editor"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "reset.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./collection-preset": {
      "import": "./dist/collection-preset.js",
      "require": "./dist/collection-preset.cjs"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.cjs"
    },
    "./creator-api": {
      "import": "./dist/creator-api.js",
      "require": "./dist/creator-api.cjs"
    },
    "./creators": {
      "import": "./dist/creators.js",
      "require": "./dist/creators.cjs"
    },
    "./editor-api": {
      "import": "./dist/editor-api.js",
      "require": "./dist/editor-api.cjs"
    },
    "./editors": {
      "import": "./dist/editors.js",
      "require": "./dist/editors.cjs"
    },
    "./exhibition-preset": {
      "import": "./dist/exhibition-preset.js",
      "require": "./dist/exhibition-preset.cjs"
    },
    "./helpers": {
      "import": "./dist/helpers.js",
      "require": "./dist/helpers.cjs"
    },
    "./manifest-preset": {
      "import": "./dist/manifest-preset.js",
      "require": "./dist/manifest-preset.cjs"
    },
    "./parser": {
      "import": "./dist/parser.js",
      "require": "./dist/parser.cjs"
    },
    "./shell": {
      "import": "./dist/shell.js",
      "require": "./dist/shell.cjs"
    },
    "./vault": {
      "import": "./dist/vault.js",
      "require": "./dist/vault.cjs"
    },
    "./package.json": "./package.json",
    "./dist/index.css": "./dist/index.css",
    "./reset.css": "./reset.css"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "publint",
    "typecheck": "tsc --noEmit",
    "prepack": "tsdown"
  },
  "dependencies": {
    "@iiif/helpers": "catalog:",
    "@iiif/parser": "catalog:",
    "@iiif/presentation-3": "^2.2.3",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@manifest-editor/collection-preset": "workspace:*",
    "@manifest-editor/components": "workspace:*",
    "@manifest-editor/creator-api": "workspace:*",
    "@manifest-editor/creators": "workspace:*",
    "@manifest-editor/editor-api": "workspace:*",
    "@manifest-editor/editors": "workspace:*",
    "@manifest-editor/exhibition-preset": "workspace:*",
    "@manifest-editor/manifest-preset": "workspace:*",
    "@manifest-editor/shell": "workspace:*",
    "@mdxeditor/editor": "^3.29.0",
    "iiif-browser": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-iiif-vault": "catalog:"
  },
  "devDependencies": {
    "@manifest-editor/ui": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "react-calendar": "^4.8.0",
    "react-clock": "^4.6.0",
    "react-datetime-picker": "^5.6.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.4.3",
    "tiny-invariant": "^1.3.3",
    "tsdown": "0.16.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "22aa937fcbd97344b3d350b75e46bb8887c56d47"
}
