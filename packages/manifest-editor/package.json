{
  "name": "manifest-editor",
  "version": "2.0.0",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/manifest-editor"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "reset.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsdown src/index.ts",
    "dev": "tsdown src/index.ts --watch",
    "lint": "publint",
    "typecheck": "tsc --noEmit",
    "prepack": "tsdown src/index.ts"
  },
  "dependencies": {
    "@iiif/helpers": "catalog:",
    "@iiif/parser": "catalog:",
    "@iiif/presentation-3": "^2.2.3",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@manifest-editor/collection-preset": "workspace:*",
    "@manifest-editor/components": "workspace:*",
    "@manifest-editor/editors": "workspace:*",
    "@manifest-editor/manifest-preset": "workspace:*",
    "@manifest-editor/shell": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-iiif-vault": "catalog:"
  },
  "devDependencies": {
    "@manifest-editor/ui": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tiny-invariant": "^1.3.3",
    "tsdown": "0.16.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "22aa937fcbd97344b3d350b75e46bb8887c56d47"
}
