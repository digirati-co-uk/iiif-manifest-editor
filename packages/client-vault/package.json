{
  "name": "@manifest-editor/client-vault",
  "version": "1.3.0",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/client-vault"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts",
    "dev": "tsdown src/index.ts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "publint",
    "prepack": "tsdown src/index.ts"
  },
  "dependencies": {
    "@iiif/helpers": "^1.4.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "tsdown": "^0.14.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "globalName": "ClientVault"
}
