{
  "name": "@manifest-editor/client-vault",
  "version": "2.0.1",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/client-vault"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown src/index.ts",
    "dev": "tsdown src/index.ts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "publint",
    "prepack": "tsdown src/index.ts"
  },
  "dependencies": {
    "@iiif/helpers": "catalog:"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "tsdown": "0.16.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "globalName": "ClientVault"
}
