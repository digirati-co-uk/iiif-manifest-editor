{
  "name": "@manifest-editor/creator-api",
  "version": "1.4.3",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/creator-api"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts",
    "dev": "tsdown src/index.ts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "publint",
    "prepack": "tsdown src/index.ts"
  },
  "dependencies": {
    "@iiif/helpers": "^1.4.0",
    "@iiif/parser": "^2.2.0",
    "@iiif/presentation-3": "^2.2.3",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@manifest-editor/editor-api": "workspace:*",
    "polygon-editor": "0.0.23",
    "react-iiif-vault": "https://pkg.csb.dev/digirati-co-uk/react-iiif-vault/commit/dc699a5a/react-iiif-vault"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@types/react": "^18.3.3",
    "tiny-invariant": "^1.3.3",
    "tsdown": "^0.14.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "22aa937fcbd97344b3d350b75e46bb8887c56d47"
}
