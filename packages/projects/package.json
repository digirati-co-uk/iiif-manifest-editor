{
  "name": "@manifest-editor/projects",
  "version": "1.0.0",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/projects"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "dev": "tsup src/index.ts --watch",
    "lint": "publint",
    "typecheck": "tsc --noEmit",
    "prepack": "tsup src/index.ts"
  },
  "dependencies": {
    "localforage": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-use-debounce": "^1.0.4",
    "immer": "^10.0.4",
    "@manifest-editor/client-vault": "1.0.0",
    "react-iiif-vault": "^1.2.1",
    "@manifest-editor/shell": "1.0.0",
    "styled-components": "6.1.8",
    "react-accessible-dropdown-menu-hook": "4.0.0",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@iiif/presentation-3": "^2.2.3",
    "@iiif/parser": "^2.1.2",
    "@iiif/helpers": "^1.2.12"
  },
  "devDependencies": {
    "slugify": "^1.6.6",
    "shallowequal": "^1.1.0",
    "@types/shallowequal": "^1.1.5",
    "@manifest-editor/ui": "1.0.0",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@repo/eslint-config": "0.0.0",
    "tiny-invariant": "^1.3.3",
    "tsup": "^8.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
