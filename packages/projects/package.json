{
  "name": "@manifest-editor/projects",
  "version": "2.0.1",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/projects"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown src/index.ts",
    "dev": "tsdown src/index.ts --watch",
    "lint": "publint",
    "typecheck": "tsc --noEmit",
    "prepack": "tsdown src/index.ts"
  },
  "dependencies": {
    "@iiif/helpers": "catalog:",
    "@iiif/parser": "catalog:",
    "@iiif/presentation-3": "^2.2.3",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@manifest-editor/client-vault": "workspace:*",
    "@manifest-editor/shell": "workspace:*",
    "immer": "^10.0.4",
    "localforage": "^1.10.0",
    "react": "catalog:",
    "react-accessible-dropdown-menu-hook": "4.0.0",
    "react-dom": "catalog:",
    "react-iiif-vault": "catalog:",
    "styled-components": "catalog:",
    "tiny-use-debounce": "^1.0.4"
  },
  "devDependencies": {
    "@manifest-editor/ui": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/shallowequal": "^1.1.5",
    "shallowequal": "^1.1.0",
    "slugify": "^1.6.6",
    "tiny-invariant": "^1.3.3",
    "tsdown": "0.16.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "22aa937fcbd97344b3d350b75e46bb8887c56d47"
}
