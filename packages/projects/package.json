{
  "name": "@manifest-editor/projects",
  "version": "1.4.3",
  "type": "module",
  "bugs": "https://github.com/digirati-co-uk/iiif-manifest-editor/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/digirati-co-uk/iiif-manifest-editor.git",
    "directory": "packages/projects"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "keywords": [
    "iiif",
    "editor",
    "typescript"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts",
    "dev": "tsdown src/index.ts --watch",
    "lint": "publint",
    "typecheck": "tsc --noEmit",
    "prepack": "tsdown src/index.ts"
  },
  "dependencies": {
    "@iiif/helpers": "^1.4.0",
    "@iiif/parser": "^2.2.0",
    "@iiif/presentation-3": "^2.2.3",
    "@iiif/presentation-3-normalized": "^0.9.7",
    "@manifest-editor/client-vault": "workspace:*",
    "@manifest-editor/shell": "workspace:*",
    "immer": "^10.0.4",
    "localforage": "^1.10.0",
    "react": "^18.3.1",
    "react-accessible-dropdown-menu-hook": "4.0.0",
    "react-dom": "^18.3.1",
    "react-iiif-vault": "https://pkg.csb.dev/digirati-co-uk/react-iiif-vault/commit/7651d8ef/react-iiif-vault",
    "styled-components": "6.1.8",
    "tiny-use-debounce": "^1.0.4"
  },
  "devDependencies": {
    "@manifest-editor/ui": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/shallowequal": "^1.1.5",
    "shallowequal": "^1.1.0",
    "slugify": "^1.6.6",
    "tiny-invariant": "^1.3.3",
    "tsdown": "^0.14.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "22aa937fcbd97344b3d350b75e46bb8887c56d47"
}
